{
  "version": "1",
  "name": "bruno-testbench",
  "type": "collection",
  "proxy": {
    "bypassProxy": "",
    "enabled": "global",
    "auth": {
      "enabled": false,
      "username": "",
      "password": ""
    },
    "port": null,
    "hostname": "",
    "protocol": "http"
  },
  "scripts": {
    "moduleWhitelist": [
      "crypto",
      "buffer"
    ],
    "filesystemAccess": {
      "allow": true
    }
  },
  "clientCertificates": {
    "enabled": true,
    "certs": []
  },
  "presets": {
    "requestType": "http",
    "requestUrl": "http://localhost:6000"
  },
  "ignore": [
    "node_modules",
    ".git"
  ]
}